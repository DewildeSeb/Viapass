    <xsd:complexType name="CopValue">
        <xsd:choice>
            <xsd:element name="noEntry" type="xsd:string"/>
            <xsd:element name="co2class1" type="xsd:string"/>
            <xsd:element name="co2class2" type="xsd:string"/>
            <xsd:element name="co2class3" type="xsd:string"/>
            <xsd:element name="co2class4" type="xsd:string"/>
            <xsd:element name="co2class5" type="xsd:string"/>
            <xsd:element name="co2class6" type="xsd:string"/>
            <xsd:element name="co2class7" type="xsd:string"/>
            <xsd:element name="reservedforUse" type="xsd:string"/>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="EnvironmentalCharacteristics">
        <xsd:sequence>
            <xsd:element name="euroValue" type="EuroValue"/>
            <xsd:element name="copValue" type="CopValue"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="EngineCharacteristics">
        <xsd:union>
            <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="noEntry"/>
                    <xsd:enumeration value="noEngine"/>
                    <xsd:enumeration value="petrolUnleaded"/>
                    <xsd:enumeration value="petrolLeaded"/>
                    <xsd:enumeration value="diesel"/>
                    <xsd:enumeration value="lPG"/>
                    <xsd:enumeration value="battery"/>
                    <xsd:enumeration value="solar"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType>
                <xsd:restriction base="xsd:unsignedByte"/>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>


    <xsd:simpleType name="DescriptiveCharacteristics">
        <xsd:union>
            <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="noEntry"/>
                    <xsd:enumeration value="vehicleShape1"/>
                    <xsd:enumeration value="vehicleShape2"/>
                    <xsd:enumeration value="vehicleShape3"/>
                    <xsd:enumeration value="vehicleShape4"/>
                    <xsd:enumeration value="vehicleShape5"/>
                    <xsd:enumeration value="vehicleShape6"/>
                    <xsd:enumeration value="vehicleShape7"/>
                    <xsd:enumeration value="vehicleShape8"/>
                    <xsd:enumeration value="vehicleShape9"/>
                    <xsd:enumeration value="vehicleShape10"/>
                    <xsd:enumeration value="vehicleShape11"/>
                    <xsd:enumeration value="vehicleShape12"/>
                    <xsd:enumeration value="vehicleShape13"/>
                    <xsd:enumeration value="vehicleShape14"/>
                    <xsd:enumeration value="vehicleShape15"/>
                    <xsd:enumeration value="vehicleShape16"/>
                    <xsd:enumeration value="vehicleShape17"/>
                    <xsd:enumeration value="vehicleShape18"/>
                    <xsd:enumeration value="vehicleShape19"/>
                    <xsd:enumeration value="vehicleShape20"/>
                    <xsd:enumeration value="vehicleShape21"/>
                    <xsd:enumeration value="vehicleShape22"/>
                    <xsd:enumeration value="vehicleShape23"/>
                    <xsd:enumeration value="vehicleShape24"/>
                    <xsd:enumeration value="vehicleShape25"/>
                    <xsd:enumeration value="vehicleShape26"/>
                    <xsd:enumeration value="vehicleShape27"/>
                    <xsd:enumeration value="vehicleShape28"/>
                    <xsd:enumeration value="vehicleShape29"/>
                    <xsd:enumeration value="vehicleShape30"/>
                    <xsd:enumeration value="vehicleShape31"/>
                    <xsd:enumeration value="vehicleShape32"/>
                    <xsd:enumeration value="vehicleShape33"/>
                    <xsd:enumeration value="vehicleShape34"/>
                    <xsd:enumeration value="vehicleShape35"/>
                    <xsd:enumeration value="vehicleShape36"/>
                    <xsd:enumeration value="vehicleShape37"/>
                    <xsd:enumeration value="vehicleShape38"/>
                    <xsd:enumeration value="vehicleShape39"/>
                    <xsd:enumeration value="vehicleShape40"/>
                    <xsd:enumeration value="vehicleShape41"/>
                    <xsd:enumeration value="vehicleShape42"/>
                    <xsd:enumeration value="vehicleShape43"/>
                    <xsd:enumeration value="vehicleShape44"/>
                    <xsd:enumeration value="vehicleShape45"/>
                    <xsd:enumeration value="vehicleShape46"/>
                    <xsd:enumeration value="vehicleShape47"/>
                    <xsd:enumeration value="vehicleShape48"/>
                    <xsd:enumeration value="vehicleShape49"/>
                    <xsd:enumeration value="vehicleShape50"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType>
                <xsd:restriction base="xsd:unsignedByte"/>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>

    <xsd:simpleType name="FutureCharacteristics">
        <xsd:union>
            <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="noEntry"/>
                    <xsd:enumeration value="airSuspension"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType>
                <xsd:restriction base="xsd:unsignedByte"/>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>

    <xsd:complexType name="VehicleSpecificCharacteristics">
        <xsd:sequence>
            <xsd:element name="environmentalCharacteristics" type="EnvironmentalCharacteristics"/>
            <!-- TODO values of engineCharacteristics have to be limited -->
            <!-- <xsd:element name="engineCharacteristics" type="EngineCharacteristics"/> -->
            <xsd:element name="engineCharacteristics" type="xsd:string"/>
            <xsd:element name="descriptiveCharacteristics" type="DescriptiveCharacteristics"/>
            <xsd:element name="futureCharacteristics" type="FutureCharacteristics"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="VehicleAuthenticator">
        <xsd:restriction base="xsd:hexBinary"/>
    </xsd:simpleType>

    <xsd:simpleType name="EquipmentOBUId">
        <xsd:restriction base="xsd:hexBinary">
            <xsd:minLength value="4"/>
            <xsd:maxLength value="4"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="EquipmentStatus">
        <xsd:restriction base="asn1:BitString">
            <xsd:length value="16"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="DriverCharacteristics">
        <xsd:sequence>
            <xsd:element name="driverClass" type="Int1"/>
            <xsd:element name="tripPurpose" type="Int1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SignedValue">
        <xsd:choice>
            <xsd:element name="positive">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:unsignedInt">
                        <xsd:minInclusive value="0"/>
                        <xsd:maxInclusive value="8388607"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="negative">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:int">
                        <xsd:minInclusive value="-8388608"/>
                        <xsd:maxInclusive value="-1"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="PaymentMeansBalance">
        <xsd:complexContent>
            <xsd:extension base="SignedValue"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:simpleType name="PaymentMeansUnit">
        <xsd:restriction base="PayUnit"/>
    </xsd:simpleType>

    <xsd:simpleType name="PaymentSecurityData">
        <xsd:restriction base="xsd:hexBinary"/>
    </xsd:simpleType>

    <xsd:simpleType name="PersonalAccountNumber">
        <xsd:restriction base="xsd:hexBinary">
            <xsd:length value="10"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="PaymentMeans">
        <xsd:sequence>
            <xsd:element name="personalAccountNumber" type="PersonalAccountNumber"/>
            <xsd:element name="paymentMeansExpiryDate" type="DateCompact"/>
            <xsd:element name="pamentMeansUsageControl">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:hexBinary">
                        <xsd:length value="2"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SessionLocation">
        <xsd:sequence>
            <xsd:element name="ascendingKilometrage" type="xsd:boolean"/>
            <xsd:element name="laneCodeNumber">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:unsignedByte">
                        <xsd:minInclusive value="0"/>
                        <xsd:maxInclusive value="127"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ReceiptData">
        <xsd:sequence>
            <xsd:element name="sessionTime" type="DateAndTime"/>
            <xsd:element name="sessionServiceProvider" type="Provider"/>
            <xsd:element name="locationOfStation" type="Int2"/>
            <xsd:element name="sessionLocation" type="SessionLocation"/>
            <xsd:element name="sessionType" type="Int1"/>
            <xsd:element name="sessionResult" type="ResultOp"/>
            <xsd:element name="sessionTariffClass" type="Int1"/>
            <xsd:element name="sessionClaimedClass" type="Int1"/>
            <xsd:element name="sessionFee" type="PaymentFee"/>
            <xsd:element name="sessionContractProvider" type="Provider"/>
            <xsd:element name="sessionTypeOfContract">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:hexBinary">
                        <xsd:length value="2"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="sessionContextVersion">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:unsignedInt">
                        <xsd:minInclusive value="0"/>
                        <xsd:maxInclusive value="127"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="receiptDataAuthenticator">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:hexBinary">
                        <xsd:length value="4"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ReceiptData1">
        <xsd:complexContent>
            <xsd:extension base="ReceiptData"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ReceiptData2">
        <xsd:complexContent>
            <xsd:extension base="ReceiptData"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ValidityOfContract">
        <xsd:sequence>
            <xsd:element name="issuerRestrictions">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:hexBinary">
                        <xsd:length value="2"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="contractExpiryDate" type="DateCompact"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PurseBalance">
        <xsd:sequence>
            <xsd:element name="purseValue" type="SignedValue"/>
            <xsd:element name="purseUnit" type="PayUnit"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ReceiptFinancialPart">
        <xsd:sequence>
            <xsd:element name="personalAccountNumber" type="PersonalAccountNumber"/>
            <xsd:element name="sessionPaymentFee" type="PaymentFee"/>
            <xsd:element name="sessionCurrentBalance" type="PurseBalance"/>
            <xsd:element name="receiptFinancialSerialNumber" type="Int4"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="SetMMIRq">
        <xsd:union>
            <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="ok"/>
                    <xsd:enumeration value="nok"/>
                    <xsd:enumeration value="contactOperator"/>
                    <xsd:enumeration value="noSignalling"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType>
                <xsd:restriction base="xsd:unsignedByte"/>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>

    <xsd:complexType name="AxleWeightLimits">
        <xsd:sequence>
            <xsd:element name="maxLadenweightOnAxle1" type="Int2"/>
            <xsd:element name="maxLadenweightOnAxle2" type="Int2"/>
            <xsd:element name="maxLadenweightOnAxle3" type="Int2"/>
            <xsd:element name="maxLadenweightOnAxle4" type="Int2"/>
            <xsd:element name="maxLadenweightOnAxle5" type="Int2"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PassengerCapacity">
        <xsd:sequence>
            <xsd:element name="numberOfSeats" type="Int1"/>
            <xsd:element name="numberOfStandingPlaces" type="Int1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Engine">
        <xsd:sequence>
            <xsd:element name="engineCapacity" type="Int2"/>
            <xsd:element name="enginePower" type="Int2"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SoundLevel">
        <xsd:sequence>
            <xsd:element name="soundstationary" type="Int1"/>
            <xsd:element name="sounddriveby" type="Int1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="UnitType">
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="mg-km"/>
            <xsd:enumeration value="mg-kWh"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="ExhaustEmissionValues">
        <xsd:sequence>
            <xsd:element name="unitType" type="UnitType"/>
            <xsd:element name="emissionCO">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:unsignedShort">
                        <xsd:minInclusive value="0"/>
                        <xsd:maxInclusive value="32767"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="emissionHC" type="Int2"/>
            <xsd:element name="emissionNOX" type="Int2"/>
            <xsd:element name="emissionHCNOX" type="Int2"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="DieselEmissionValues">
        <xsd:sequence>
            <xsd:element name="particulate">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="unitType" type="UnitType"/>
                        <xsd:element name="value">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedShort">
                                    <xsd:minInclusive value="0"/>
                                    <xsd:maxInclusive value="32767"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="absorptionCoeff" type="Int2"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="CO2EmissionValue">
        <xsd:restriction base="Int2"/>
    </xsd:simpleType>

    <xsd:simpleType name="VehicleTotalDistance">
        <xsd:restriction base="Int4"/>
    </xsd:simpleType>

    <xsd:complexType name="TrailerLicencePlateNumber">
        <xsd:complexContent>
            <xsd:extension base="LPN"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="TrailerDetails">
        <xsd:sequence>
            <xsd:element name="trailerType">
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:token">
                                <xsd:enumeration value="notPresent"/>
                                <xsd:enumeration value="trailer"/>
                                <xsd:enumeration value="semitrailer"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:unsignedByte">
                                <xsd:minInclusive value="0"/>
                                <xsd:maxInclusive value="31"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="trailerAxles" type="TrailerAxles"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="TrailerCharacteristics">
        <xsd:sequence>
            <xsd:element name="trailerDetails" type="TrailerDetails"/>
            <xsd:element name="trailerMaxLadenWeight" type="Int2"/>
            <xsd:element name="trailerWeightUnladen" type="Int2"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="ActualNumberOfPassengers">
        <xsd:restriction base="Int1"/>
    </xsd:simpleType>

    <xsd:complexType name="VehicleLicencePlateNumber">
        <xsd:complexContent>
            <xsd:extension base="LPN"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:simpleType name="VehicleIdentificationNumber">
        <xsd:restriction base="AVIAEINumberingAndDataStructures:CS5"/>
    </xsd:simpleType>

</xsd:schema>