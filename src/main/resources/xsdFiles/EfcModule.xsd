            <xsd:element name="vehicleFirstAxleHeight" type="Int1"/>
            <xsd:element name="vehicleAxlesNumber">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="tyreType">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:token">
                                    <xsd:enumeration value="notSpecified"/>
                                    <xsd:enumeration value="singleTyre"/>
                                    <xsd:enumeration value="dualTyres"/>
                                    <xsd:enumeration value="reservedForUse"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="numberOfAxles">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="trailerAxles" type="TrailerAxles"/>
                                    <xsd:element name="tractorAxles" type="TractorAxles"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="VehicleWeightLimits">
        <xsd:sequence>
            <xsd:element name="vehicleMaxLadenWeight" type="Int2"/>
            <xsd:element name="vehicleTrainMaximumWeight" type="Int2"/>
            <xsd:element name="vehicleWeightUnladen" type="Int2"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="VehicleWeightLaden">
        <xsd:restriction base="Int2"/>
    </xsd:simpleType>

