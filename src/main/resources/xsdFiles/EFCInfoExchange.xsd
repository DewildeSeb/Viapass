                                <xsd:restriction base="xsd:hexBinary">
                                    <xsd:length value="14"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="anprConfidence">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedByte">
                                    <xsd:minInclusive value="0"/>
                                    <xsd:maxInclusive value="100"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="secondaryAnprResult" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:hexBinary">
                                    <xsd:length value="14"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="manualResult" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:hexBinary">
                                    <xsd:length value="14"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="operatorId">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedShort"/>
                            </xsd:simpleType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="vehicleDetails">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="vehicleMake" minOccurs="0" type="xsd:string"/>
                        <xsd:element name="vehicleModel" minOccurs="0" type="xsd:string"/>
                        <xsd:element name="vehicleColour" minOccurs="0" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="subRecordAuthenticator" minOccurs="0" type="SubRecordAuthenticator"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ClassificationRecord">
        <xsd:sequence>
            <xsd:element name="classificationRecordId" type="RecordId"/>
            <xsd:element name="derivedLocalClass">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:unsignedByte"/>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="measuredParameters">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="measurementTime" type="xsd:dateTime"/>
                        <xsd:element name="measuredClass" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedByte"/>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="measuredVehicleLength" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedShort"/>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="measuredVehicleWidth" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedShort"/>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="measuredVehicleHeight" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedShort"/>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="measuredHeightAbove1stAxle" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedShort"/>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="measuredVehicleAxles" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedByte">
                                    <xsd:minInclusive value="0"/>
                                    <xsd:maxInclusive value="15"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="measuredTrailerAxles" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedByte">
                                    <xsd:minInclusive value="0"/>
                                    <xsd:maxInclusive value="7"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="measuredTrailerPresence" minOccurs="0" type="xsd:boolean"/>
                        <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="subRecordAuthenticator" minOccurs="0" type="SubRecordAuthenticator"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OperatorRecord">
        <xsd:sequence>
            <xsd:element name="operatorRecordId" type="RecordId"/>
            <xsd:element name="operatorData">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="operatorTime" type="xsd:dateTime"/>
                        <xsd:element name="operatorClass" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedByte"/>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="operatorId" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:unsignedShort"/>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="operatorVRM" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:hexBinary">
                                    <xsd:length value="14"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="operatorPAN" minOccurs="0" type="xsd:string"/>
                        <xsd:element name="operatorOBUId" minOccurs="0" type="xsd:string"/>
                        <xsd:element name="machineReadPAN" minOccurs="0" type="xsd:string"/>
                        <xsd:element name="machineReadOBUId" minOccurs="0" type="xsd:string"/>
                        <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="subRecordAuthenticator" minOccurs="0" type="SubRecordAuthenticator"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AssociatedEventData">
        <xsd:sequence>
            <xsd:element name="cCCRecord" minOccurs="0" type="CCCEvent"/>
            <xsd:element name="imageRecord" minOccurs="0" type="ImageRecord"/>
            <xsd:element name="aNPRRecord" minOccurs="0" type="ANPRRecord"/>
            <xsd:element name="classificationRecord" minOccurs="0" type="ClassificationRecord"/>
            <xsd:element name="operatorRecord" minOccurs="0" type="OperatorRecord"/>
            <xsd:element name="dsrcData" minOccurs="0" type="DSRCData"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="BillingDetailsADU">
        <xsd:sequence>
            <xsd:element name="billingDetailsId">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="issuerId" type="EfcModule:Provider"/>
                        <xsd:element name="claimId" type="xsd:integer"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="tollChargerId" type="EfcModule:Provider"/>
            <xsd:element name="contextId" type="RegimeID"/>
            <xsd:element name="userId" minOccurs="0" type="UserId"/>
            <xsd:element name="period" type="Period"/>
            <xsd:element name="billingDetailsAmount" type="Amount"/>
            <xsd:element name="usageDetails" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="contextName" type="xsd:string"/>
                        <xsd:element name="appliedUserClass" type="xsd:string"/>
                        <xsd:element name="perDeclaredVehicleClasses">
                            <xsd:complexType>
                                <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                    <xsd:element name="SEQUENCE">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="declaredVehicleClass" type="xsd:string"/>
                                                <xsd:element name="perUsedTimeClasses">
                                                    <xsd:complexType>
                                                        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                                            <xsd:element name="SEQUENCE">
                                                                <xsd:complexType>
                                                                    <xsd:sequence>
                                                                        <xsd:element name="appliedTimeClass"
                                                                                     type="xsd:string"/>
                                                                        <xsd:element name="costCenter" minOccurs="0"
                                                                                     type="xsd:string"/>
                                                                        <xsd:element name="usageList">
                                                                            <xsd:complexType>
                                                                                <xsd:sequence minOccurs="0"
                                                                                              maxOccurs="unbounded">
                                                                                    <xsd:element name="UsageDetail"
                                                                                                 type="UsageDetail"/>
                                                                                </xsd:sequence>
                                                                            </xsd:complexType>
                                                                        </xsd:element>
                                                                    </xsd:sequence>
                                                                </xsd:complexType>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="refTollDeclaration" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                        <xsd:element name="TollDeclarationId" type="TollDeclarationId"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="associatedEventData" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                        <xsd:element name="AssociatedEventData" type="AssociatedEventData"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PaymentClaimADU">
        <xsd:sequence>
            <xsd:element name="paymentClaimId" type="xsd:integer"/>
            <xsd:element name="startDateTime" type="xsd:dateTime"/>
            <xsd:element name="endDateTime" minOccurs="0" type="xsd:dateTime"/>
            <xsd:element name="userId" minOccurs="0" type="UserId"/>
            <xsd:element name="paymentClaimAmount" type="Amount"/>
            <xsd:element name="paymentClaimStatus" type="PaymentClaimStatus"/>
            <xsd:element name="typeOfGoods" minOccurs="0" type="TypeOfGoods"/>
            <xsd:element name="associatedBillingDetails" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                        <xsd:element name="RelatedMessageId" type="RelatedMessageId"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ReportQAADU">
        <xsd:sequence>
            <xsd:element name="qualityParameterID" type="xsd:integer"/>
            <xsd:element name="qualityParameterName" minOccurs="0" type="xsd:string"/>
            <xsd:element name="qualityParameterValue" minOccurs="0" type="xsd:double"/>
            <xsd:element name="qualityParameterStatus" type="QualityParameterStatus"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="StatusADU">
        <xsd:choice>
            <xsd:element name="generalStatusCode" type="GeneralStatusCode"/>
            <xsd:element name="messageStatusCode" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="message" type="MessageIdentifier"/>
                        <xsd:element name="reasonOfADUStruct">
                            <xsd:complexType>
                                <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                    <xsd:element name="SEQUENCE">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="numberOfADUStruct" type="NumberOfADUStruct"/>
                                                <xsd:element name="reason" type="MessageReasonCode"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="ListOfParametersRequested">
        <xsd:sequence minOccurs="1" maxOccurs="7">
            <xsd:element name="UserParameterRequest">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RetrieveUserDetails">
        <xsd:sequence>
            <xsd:element name="userId" type="UserId"/>
            <xsd:element name="listOfParametersRequested" minOccurs="1" type="ListOfParametersRequested"/>
            <xsd:element name="userDetailsRequestReason" minOccurs="0" type="UserDetailsRequestReason"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RetrieveUserDetailsADU">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="RetrieveUserDetails" type="RetrieveUserDetails"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="name">
        <xsd:sequence>
            <xsd:element name="name">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="60"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="UserPostalAddress">
        <xsd:sequence>
            <xsd:element name="name">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="60"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="company" minOccurs="0">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="60"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="street" minOccurs="0">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="60"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="postcode">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="10"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="town" minOccurs="0">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="60"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="country">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="40"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <!-- <xsd:any namespace="##other" processContents="lax" minOccurs="0"/> -->
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="UserParameterResponseDetails">
        <xsd:choice>
            <xsd:element name="userPostalAddress" type="UserPostalAddress"/>
            <xsd:element name="contractSerialNumber" type="EfcModule:ContractSerialNumber"/>
            <xsd:element name="contractValidity" type="EfcModule:ContractValidity"/>
            <xsd:element name="driverCharacteristics" type="EfcModule:DriverCharacteristics"/>
            <xsd:element name="eFC-ContextMark" type="EfcModule:EFC-ContextMark"/>
            <xsd:element name="environmentalCharacteristics" type="EfcModule:EnvironmentalCharacteristics"/>
            <xsd:element name="engineCharacteristics" type="EfcModule:EngineCharacteristics"/>
            <xsd:element name="equipmentOBUId" type="EfcModule:EquipmentOBUId"/>
            <xsd:element name="equipmentStatus" type="EfcModule:EquipmentStatus"/>
            <xsd:element name="paymentMeans" type="EfcModule:PaymentMeans"/>
            <xsd:element name="paymentMeansBalance" type="EfcModule:PaymentMeansBalance"/>
            <xsd:element name="paymentMeansUnit" type="EfcModule:PaymentMeansUnit"/>
            <xsd:element name="personalAccountNumber" type="EfcModule:PersonalAccountNumber"/>
            <xsd:element name="provider" type="EfcModule:Provider"/>
            <xsd:element name="receiptContract" type="EfcModule:ReceiptContract"/>
            <xsd:element name="validityOfContract" type="EfcModule:ValidityOfContract"/>
            <xsd:element name="vehicleAuthenticator" type="EfcModule:VehicleAuthenticator"/>
            <xsd:element name="vehicleClass" type="EfcModule:VehicleClass"/>
            <xsd:element name="vehicleDimensions" type="EfcModule:VehicleDimensions"/>
            <xsd:element name="vehicleLicencePlateNumber" type="EfcModule:VehicleLicencePlateNumber"/>
            <xsd:element name="vehicleIdentificationNumber" type="EfcModule:VehicleIdentificationNumber"/>
            <xsd:element name="vehicleWeightLaden" type="EfcModule:VehicleWeightLaden"/>
            <xsd:element name="vehicleWeightLimits" type="EfcModule:VehicleWeightLimits"/>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="UserParameterResponse">
        <xsd:sequence>
            <xsd:element name="userParameterResponse" type="UserParameterResponseDetails"/>
            <xsd:element name="userParameterStatus" type="UserParameterStatus"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="listOfUserParameters">
        <xsd:sequence minOccurs="1" maxOccurs="7">
            <xsd:element name="UserParameterResponse" type="UserParameterResponse"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ProvideUserDetails">
        <xsd:sequence>
            <xsd:element name="originaluserIdRequest" type="UserId"/>
            <xsd:element name="userId" type="UserId"/>
            <xsd:element name="statusFlag" minOccurs="0" type="UserStatus"/>
            <xsd:element name="listOfUserParameters" minOccurs="0" maxOccurs="1" type="listOfUserParameters"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ProvideUserDetailsADU">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="ProvideUserDetails" type="ProvideUserDetails"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ReportCCCEventADU">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="CCCEvent" type="CCCEvent"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RetrieveTollDeclarations">
        <xsd:sequence>
            <xsd:element name="userID" minOccurs="0" type="UserId"/>
            <xsd:element name="startTime" minOccurs="1" type="asn1:GeneralizedTime"/>
            <xsd:element name="endTime" minOccurs="1" type="asn1:GeneralizedTime"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RetrieveTollDeclarationADU">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="RetrieveTollDeclarations" type="RetrieveTollDeclarations"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RetrieveCCCEvents">
        <xsd:sequence>
            <xsd:element name="userID" minOccurs="0" type="UserId"/>
            <xsd:element name="startTime" minOccurs="0" type="xsd:dateTime"/>
            <xsd:element name="endTime" minOccurs="0" type="xsd:dateTime"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RetrieveCCCEventADU">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="RetrieveCCCEvents" type="RetrieveCCCEvents"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="aduType">
        <xsd:choice>
            <xsd:element name="requestADU" type="RequestADU"/>
            <xsd:element name="ackADU" type="AckADU"/>
            <xsd:element name="trustObjectADU" type="TrustObjectADU"/>
            <xsd:element name="efcContextDataADU" type="EFCContextDataADU"/>
            <xsd:element name="exceptionListADU" type="ExceptionListADU"/>
            <xsd:element name="reportAbnormalOBEADU" type="ReportAbnormalOBEADU"/>
            <xsd:element name="tollDeclarationADU" type="TollDeclarationADU"/>
            <xsd:element name="billingDetailsADU" type="BillingDetailsADU"/>
            <xsd:element name="paymentClaimADU" type="PaymentClaimADU"/>
            <xsd:element name="reportQAADU" type="ReportQAADU"/>
            <xsd:element name="statusADU" type="StatusADU"/>
            <xsd:element name="retrieveUserDetailsADU" type="RetrieveUserDetailsADU"/>
            <xsd:element name="provideUserDetailsADU" type="ProvideUserDetailsADU"/>
            <xsd:element name="reportCCCEventADU" type="ReportCCCEventADU"/>
            <xsd:element name="retrieveTollDeclarationADU" type="RetrieveTollDeclarationADU"/>
            <xsd:element name="retrieveCCCEventADU" type="RetrieveCCCEventADU"/>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="InfoExchangeContent">
        <xsd:sequence>
            <xsd:element name="apci" type="ApciFields"/>
            <xsd:element name="adu" type="aduType"/>
        </xsd:sequence>
    </xsd:complexType>

</xsd:schema>