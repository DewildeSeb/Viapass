<?xml version="1.0" encoding="UTF-8"?><!--this .xsd file is use to define each type of element in all request/response in viapass
it contain the simpleType and the simplest ComplexType
this .xsd is base on document Back-office-communication 2.6 28-02-2018,
this document must resume the european standard iso-12855-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:x="asn1Namespace" targetNamespace="asn1Namespace">

    <xsd:simpleType name="BitString">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-1]{0,}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="NumberOfADUStruct">
        <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="1"/>
            <xsd:maxInclusive value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="CountryCode">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-1]{0,}"/>
            <xsd:length value="10"/>
            <!--            <xsd:minLength value="10"/>-->
            <!--            <xsd:maxLength value="10"/>-->
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="RegimeID">
        <xsd:restriction base="xsd:unsignedShort"/>
    </xsd:simpleType>
    <xsd:simpleType name="MessageIdentifier">
        <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="0"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ProviderIdentifier">
        <xsd:restriction base="xsd:nonNegativeInteger">
            <xsd:maxInclusive value="16383"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="NULL" final="#all"/>
    <xsd:simpleType name="OctetString">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9a-fA-F]{0,}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ManufacturerId">
        <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="65535"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="LPNLimited">
        <xsd:restriction base="xsd:hexBinary">
            <xsd:minLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="GeneralizedTime">
        <xsd:restriction base="xsd:string">
            <!-- <xsd:pattern  value="\d{14}[,\.]?\d*[+-]\d{4}" /> -->
            <xsd:pattern value="\d{14}[,\.]?\d*Z"/>
            <!-- <xsd:pattern  value="\d{14}[,\.]?\d*" /> -->
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ContractSerialNumber">
        <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="4294967295"/>
            <xsd:pattern value="[0-9]+"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="IssuerIdentifier">
        <xsd:restriction base="xsd:nonNegativeInteger">
            <!--<xsd:minInclusive value="0"/>-->
            <xsd:maxInclusive value="16383"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="EquipmentOBUId">
        <xsd:restriction base="xsd:hexBinary">
            <!--            <xsd:minLength value="4"/>-->
            <!--            <xsd:maxLength value="4"/>-->
            <xsd:length value="4"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="PersonalAccountNumber">
        <xsd:restriction base="xsd:hexBinary">
            <xsd:length value="10"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="TrailerAxles">
        <xsd:restriction base="xsd:unsignedByte">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="7"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="TractorAxles">
        <xsd:restriction base="xsd:unsignedByte">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="7"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ResultFin">
        <xsd:restriction base="xsd:hexBinary">
            <xsd:length value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="AttributeIdList">
        <xsd:restriction>
            <xsd:simpleType>
                <xsd:list itemType="x:UnsignedInt0_127"/>
            </xsd:simpleType>
            <xsd:minLength value="0"/>
            <xsd:maxLength value="127"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="UnsignedInt0_127">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="127"/>
        </xsd:restriction>
    </xsd:simpleType>
    <!--    <xsd:simpleType name="ActionType">-->
    <!--        <xsd:restriction base="xsd:unsignedInt">-->
    <!--            <xsd:minInclusive value="0"/>-->
    <!--            <xsd:maxInclusive value="127"/>-->
    <!--        </xsd:restriction>-->
    <!--    </xsd:simpleType>-->
    <xsd:simpleType name="HexBinaryLength2">
        <xsd:restriction base="xsd:hexBinary">
            <xsd:length value="2"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="HexBinaryLength4">
        <xsd:restriction base="xsd:hexBinary">
            <xsd:length value="4"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Dist">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="16777215"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Positive">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="8388607"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Negative">
        <xsd:restriction base="xsd:int">
            <xsd:minInclusive value="-8388608"/>
            <xsd:maxInclusive value="-1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="AccountStatus">
        <xsd:union memberTypes="xsd:token">
            <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="ok"/>
                    <xsd:enumeration value="low"/>
                    <xsd:enumeration value="empty"/>
                    <xsd:enumeration value="negative"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>
    <xsd:simpleType name="TrustObjectStatus">
        <xsd:union>
            <xsd:simpleType>
                <xsd:restriction base="xsd:token">
                    <xsd:enumeration value="valid"/>
                    <xsd:enumeration value="expired"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType>
                <xsd:restriction base="xsd:integer"/>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>

