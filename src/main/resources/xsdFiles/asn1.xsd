            <xsd:element name="noEntry" type="xsd:string"/>
            <xsd:element name="euro-1" type="xsd:string"/>
            <xsd:element name="euro-2" type="xsd:string"/>
            <xsd:element name="euro-3" type="xsd:string"/>
            <xsd:element name="euro-4" type="xsd:string"/>
            <xsd:element name="euro-5" type="xsd:string"/>
            <xsd:element name="euro-6" type="xsd:string"/>
            <xsd:element name="reservedForUse1" type="xsd:string"/>
            <xsd:element name="reservedForUse2" type="xsd:string"/>
            <xsd:element name="reservedForUse3" type="xsd:string"/>
            <xsd:element name="reservedForUse4" type="xsd:string"/>
            <xsd:element name="reservedForUse5" type="xsd:string"/>
            <xsd:element name="reservedForUse6" type="xsd:string"/>
            <xsd:element name="reservedForUse7" type="xsd:string"/>
            <xsd:element name="reservedForUse8" type="xsd:string"/>
            <xsd:element name="reservedForUse9" type="xsd:string"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="AduTypeString">
        <xsd:choice>
            <xsd:element name="requestADU" type="xsd:string"/>
            <xsd:element name="ackADU" type="xsd:string"/>
            <xsd:element name="trustObjectADU" type="xsd:string"/>
            <xsd:element name="efcContextDataADU" type="xsd:string"/>
            <xsd:element name="exceptionListADU" type="xsd:string"/>
            <xsd:element name="reportAbnormalOBEADU" type="xsd:string"/>
            <xsd:element name="tollDeclarationADU" type="xsd:string"/>
            <xsd:element name="billingDetailsADU" type="xsd:string"/>
            <xsd:element name="paymentClaimADU" type="xsd:string"/>
            <xsd:element name="reportQAADU" type="xsd:string"/>
            <xsd:element name="statusADU" type="xsd:string"/>
            <xsd:element name="retrieveUserDetailsADU" type="xsd:string"/>
            <xsd:element name="provideUserDetailsADU" type="xsd:string"/>
            <xsd:element name="reportCCCEventADU" type="xsd:string"/>
            <xsd:element name="retrieveTollDeclarationADU" type="xsd:string"/>
            <xsd:element name="retrieveCCCEvents" type="xsd:string"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="ADUType">
        <xsd:choice>
            <xsd:element name="requestADU"/>
            <xsd:element name="ackADU"/>
            <xsd:element name="trustObjectADU"/>
            <xsd:element name="efcContextDataADU"/>
            <xsd:element name="exceptionListADU"/>
            <xsd:element name="reportAbnormalOBEADU"/>
            <xsd:element name="tollDeclarationADU"/>
            <xsd:element name="billingDetailsADU"/>
            <xsd:element name="paymentClaimADU"/>
            <xsd:element name="reportQAADU"/>
            <xsd:element name="statusADU"/>
            <xsd:element name="retrieveUserDetailsADU"/>
            <xsd:element name="provideUserDetailsADU"/>
            <xsd:element name="reportCCCEventADU"/>
            <xsd:element name="retrieveTollDeclarationADU"/>
            <xsd:element name="retrieveCCCEventADU"/>
        </xsd:choice>
