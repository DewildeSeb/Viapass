    <xsd:complexType name="DisUnit">
        <xsd:choice>
            <xsd:element name="kilometres" type="xsd:string"/>
            <xsd:element name="miles" type="xsd:string"/>
            <xsd:element name="metres" type="xsd:string"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="ExceptionListReasonType">
        <xsd:choice>
            <xsd:element name="notToBeDisclosed" type="xsd:string"/>
            <xsd:element name="obeDeactivated" type="xsd:string"/>
            <xsd:element name="obeIsStolen" type="xsd:string"/>
            <xsd:element name="temporaryTechnicalProblem" type="xsd:string"/>
            <xsd:element name="suspicionOnTechnicalManipulation" type="xsd:string"/>
            <xsd:element name="latePayment" type="xsd:string"/>
            <xsd:element name="noPayment" type="xsd:string"/>
            <xsd:element name="contractHolderInsolvent" type="xsd:string"/>
