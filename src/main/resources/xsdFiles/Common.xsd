<xsd:schema attributeFormDefault="qualified" targetNamespace="CommonNamespace" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:asn1="asn1Namespace" xmlns:x="CommonNamespace">

    <xsd:import namespace="asn1Namespace" schemaLocation="asn1.xsd"/>


    <xsd:complexType name="Distance">
        <xsd:sequence>
            <xsd:element name="dist" type="asn1:Dist"/>
            <xsd:element name="disUnit" type="asn1:DisUnit"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Period">
        <xsd:sequence>
            <xsd:element name="beginOfPeriod" type="asn1:DateAndTime"/>
            <xsd:element name="endOfPeriod" type="asn1:DateAndTime"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LicensePlateNumber">
        <xsd:sequence>
            <xsd:element name="countryCode" type="asn1:CountryCode"/>
